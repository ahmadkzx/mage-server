{
  "verbose": true,
  "ext": ".ts,.js",
  "restartable": "rs",
  "runOnChangeOnly": false,
  "exec": "npx ts-node ./src/index.ts",
  "ignore": [
    ".git",
    "dist",
    "node_modules/**/node_modules"
  ],
  "execMap": {
    "js": "node"
  },
  "watch": [
    "src"
  ],
  "env": {
    "NODE_ENV": "development"
  }
}